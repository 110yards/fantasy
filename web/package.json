{
  "name": "web",
  "version": "3.0.0",
  "private": false,
  "scripts": {
    "serve": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve --mode dev",
    "build": "vue-cli-service build",
    "build:test": "vue-cli-service build --mode test",
    "build:live": "vue-cli-service build --mode live",
    "format-check": "prettier --check src/**/*.{js,vue}",
    "format": "prettier --write src/**/*.{js,vue}",
    "test": "jest --detectOpenHandles --testPathIgnorePatterns=.*/ui/.* --passWithNoTests",
    "testAll": "jest --detectOpenHandles",
    "ui-test": "jest --detectOpenHandles --testPathPatterns=.*/ui/.*"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "axios": "^0.24.0",
    "date-fns": "^2.23.0",
    "firebase": "^8.10.0",
    "vue": "^2.6.10",
    "vue-loading-overlay": "^3.4.2",
    "vue-router": "^3.1.3",
    "vuedraggable": "^2.23.2",
    "vuefire": "^2.2.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.0.1",
    "vuexfire": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/runtime-dom": "^3.2.23",
    "fs": "^0.0.1-security",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "sass": "^1.32.13",
    "sass-loader": "^10.2.0",
    "selenium-webdriver": "^4.0.0",
    "vue-cli-plugin-vuetify": "^2.4.4",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.7.3"
  },
  "license": "MIT"
}
