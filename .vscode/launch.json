{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "API",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "console": "internalConsole",
      "justMyCode": false,
      "args": [
        "api.app.main:app",
        "--app-dir",
        "..",
        "--reload",
        "--reload-dir",
        "api",
        "--port",
        "8000",
        "--env-file",
        "./api/.env"
      ],
      "jinja": true
    },
    {
      "name": "System",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "console": "internalConsole",
      "args": [
        "services.system.app.main:app",
        "--app-dir",
        "..",
        "--reload",
        "--reload-dir",
        "services/system",
        "--reload-dir",
        "yards_py",
        "--port",
        "8001",
        "--env-file",
        "./services/system/.env"
      ],
      "jinja": true
    }
  ]
}
